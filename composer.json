{
    "name": "yusuftogtay/generator",
    "type": "library",
    "description": "InfyOm Laravel Generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/support": "^11.0",
        "illuminate/console": "^11.0",
        "laracasts/flash": "^3.2.2",
        "laravelcollective/html": "1.0.0",
        "symfony/var-exporter": "^6.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4.3",
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^9.5.2",
        "pestphp/pest": "3.x-dev",
        "pestphp/pest-plugin-laravel": "3.x-dev"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/yusuftogtay/html"
        }
    ],
    "autoload": {
        "psr-4": {
            "YusufTogtay\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\YusufTogtay\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/infyomlabs"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
